{
	 "title": "Study Parameters",
	 "type": "object",
	 "properties": {
	   "Researcher Name": {
	     "type": "string",
	     "description": "First and Last name",
	     "minLength": 4,
	     "default": "Jeremy Dorn"
	   },
	   "Researcher ID": {
	     "type": "integer",
	     "default": 25,
	     "minimum": 18,
	     "maximum": 99
	   },
	   "Study Name": {
	     "type": "string",
	     "format": "color",
	     "title": "favorite color",
	     "default": "#ffa500"
	   },
	   "gender": {
	     "type": "string",
	     "enum": [
	   "male",
	   "female"
	     ]
	   },
	   "studyDef": {
	     "type": "object",
	     "title": "Study Definition",
	     "properties": {
	   "city": {
	     "type": "string",
	     "default": "San Francisco"
	   },
	   "state": {
	     "type": "string",
	     "default": "CA"
	   },
	   "citystate": {
	     "type": "string",
	     "description": "This is generated automatically from the previous two fields",
	     "template": "{{city}}, {{state}}",
	     "watch": {
	       "city": "location.city",
	       "state": "location.state"
	     }
	   }
	     }
	   },
	   "params": {
	     "type": "array",
	     "format": "tabs",
	     "title": "Parâmetros",
	     "items": {
	   "type": "object",
	   "title": "Group",
	   "headerTemplate":"{{ i1 }} - {{self.type}}",
	   "properties": {
	     "type": {
	       "title": "X",
	       "type": "string",
	       "enum": [
	         "Características",
	         "Seleção",
	         "Evento Subsequente"
	       ],
	       "default": "Características"
	     }
	   }
	     },
	     "default": [
	   {
	     "type": "Características"
	   },
	   {
	     "type": "Seleção"
	   },
	   {
	     "type": "Evento Subsequente"
	   }
	     ]
	   }
	 }
}